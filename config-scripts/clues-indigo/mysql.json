{
  "id": "mysql",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mysql",
      "network": "BRIDGE",
      "portMappings": [{
        "containerPort": 3306,
        "servicePort": 8306,
        "protocol": "tcp"
      }]
    }
  },
  "env": {
    "MYSQL_ROOT_PASSWORD": "password"
  },
  "instances": 1,
  "cpus": 0.5,
  "mem": 256
}
